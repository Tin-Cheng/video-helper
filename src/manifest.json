{
  "manifest_version": 3,
  "name": "Video-Helper",
  "description": "Helps adjust plauback speed, AB loop and jump forward / backward",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ],
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage",
    "commands"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "commands": {
    "toggle-feature-loop": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      },
      "description": "Toggle feature loop"
    },
    "toggle-feature-jump-forward": {
      "suggested_key": {
        "default": "Ctrl+Shift+O",
        "mac": "Command+Shift+O"
      },
      "description": "Toggle feature jump forward"
    },
    "toggle-feature-jump-backward": {
      "suggested_key": {
        "default": "Ctrl+Shift+I",
        "mac": "Command+Shift+I"
      },
      "description": "Toggle feature jump backward"
    }
  }
}